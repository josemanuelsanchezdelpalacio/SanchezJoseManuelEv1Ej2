<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd"
             version="3.0">
  <persistence-unit name="default">
      <class>entities.EmpleadoEntity</class>
      <class>entities.MaquinariaEntity</class>
      <class>entities.ObraEntity</class>

      <properties>
          <property name="toplink.jdbc.url" value="jdbc:mariadb://localhost:3909/constructoraH"/>
          <property name="toplink.jdbc.driver" value="org.mariadb.jdbc.Driver"/>
